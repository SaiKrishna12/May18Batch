package day24;

public class DoubleDimensionalArrays {

	/**
	 * @param args
	 */
	public static void main(String[] args) {
		String[][] str=new String[2][3];
		str[0][0]="selenium";
		str[0][1]="qtp";
		str[0][2]="lr";
		
		str[1][0]="jemter";
		str[1][1]="codedui";
		str[1][2]="sopaui";
		
		
		for(int i=0;i<2;i++)
		{
			for(int j=0;j<3;j++)
			{
				System.out.print(str[i][j]+"   ");
			}
			System.out.println();
		}

	}

}
